  </servlet>
  <servlet-mapping>
    <servlet-name>GetAllShares</servlet-name>
    <url-pattern>/get</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Searchshares</display-name>
